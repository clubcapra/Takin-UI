{
  "name": "capra_web_ui",
  "version": "2.0.3",
  "author": {
    "name": "Club Capra",
    "email": "admin@clubcapra.com",
    "url": "http://www.clubcapra.com"
  },
  "description": "User Interface for ROS based robots",
  "private": true,
  "homepage": "./",
  "main": "build/electron/main.js",
  "scripts": {
    "start": "concurrently -k -s first -n react,electron npm:start:react npm:start:electron",
    "start:react": "react-app-rewired start",
    "start:electron": "npm run build:electron:ts && wait-on http://localhost:3000 && electron .",
    "build": "concurrently npm:build:react npm:build:electron:ts",
    "build:react": "react-app-rewired build",
    "build:electron": "npm run build && electron-builder",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "lint": "eslint --ext js,ts,tsx src",
    "fix": "eslint --fix --ext js,ts,tsx src",
    "typecheck": "tsc --noEmit",
    "build:electron:ts": "cd ./src/electron/ && tsc",
    "check": "concurrently npm:lint npm:typecheck"
  },
  "build": {
    "appId": "com.clubcapra.capra_web_ui",
    "files": [
      "build/**/*"
    ],
    "linux": {
      "target": [
        "deb"
      ]
    },
    "directories": {
      "buildResources": "./src/assets/build"
    },
    "artifactName": "capra_web_ui_setup.${ext}",
    "extends": null
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.3.4",
    "@types/roslib": "1.0.0",
    "@xstate/react": "^0.8.1",
    "chart.js": "^2.9.3",
    "chartjs-plugin-streaming": "^1.8.0",
    "date-fns": "^2.12.0",
    "lodash": "^4.17.15",
    "polished": "^3.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.9.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-toastify": "^5.5.0",
    "redux": "^4.0.5",
    "ros3d": "^1.0.1",
    "roslib": "^1.1.0",
    "shortid": "^2.2.15",
    "styled-components": "^5.1.0",
    "typescript": "3.8.3",
    "xstate": "^4.7.8"
  },
  "devDependencies": {
    "@types/chart.js": "^2.9.19",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.25",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/roslib": "1.0.0",
    "@types/shortid": "0.0.29",
    "@types/styled-components": "^5.0.1",
    "concurrently": "^5.1.0",
    "electron": "^8.2.1",
    "electron-builder": "^22.4.1",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.4",
    "react-app-rewired": "^2.1.5",
    "react-scripts": "3.4.1",
    "wait-on": "^4.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
